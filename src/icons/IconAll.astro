export const ICONS = {
  angular: IconAngular,
  astro: IconAstro,
  css: IconCss,
  dart: IconDart,
  firebase: IconFirebase,
  flutter: IconFlutter,
  git: IconGit,
  github: IconGithub,
  html: IconHtml,
  ionic: IconIonic,
  javascript: IconJavascript,
